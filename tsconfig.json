{
  "extends": "./tsconfig-autogen.json",
  "compilerOptions": {
    "outDir": "./dist/",
    "moduleResolution": "node",
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "baseUrl": "./"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
